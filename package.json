{
  "name": "ezmlm",
  "version": "0.1.7",
  "description": "Ezmlm wrapper for Node.js",
  "main": "lib/ezmlm.js",
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint src/",
    "test": "npm run lint && ./node_modules/.bin/mocha --reporter spec --compilers coffee:coffee-script/register spec/*.spec.coffee",
    "prepublish": "./node_modules/.bin/coffee -o lib/ -c src/*.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flosse/node-ezmlm.git"
  },
  "keywords": [
    "mailinglist",
    "manager",
    "mail",
    "ezmlm"
  ],
  "author": "Markus Kohlhase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flosse/node-ezmlm/issues"
  },
  "homepage": "https://github.com/flosse/node-ezmlm",
  "devDependencies": {
    "chai": "^2.3.0",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.14.2",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "async": "1.5.1",
    "mkdirp": "^0.5.1",
    "sane": "^1.3.0"
  }
}
